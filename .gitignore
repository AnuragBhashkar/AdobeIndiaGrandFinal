# yarn.lock
# package-lock.json


# --- Backend (Python / FastAPI) ---
# Ignore Python virtual environment folders
/Backend/venv/
/Backend/env/
/Backend/.venv/
/Backend/.env/

# Ignore Python bytecode and cache
/Backend/__pycache__/
